<section class="orders-show">

  <%= form_for :user, url: '/users' do |f| %>
    <fieldset>
      <legend>Signup > Account Details</legend>
      <%= form_for :user, url: '/users' do |f| %>
        <div>
          <label for="first_name">First Name</label>
          <%= tag.p f.text_field :first_name, id: "first_name" %>
        </div>
        <div>
          <label for="last_name">Last Name</label>
          <%= tag.p f.text_field :last_name, id: "last_name" %>
        </div>
        <div>
          <label for="email">Email</label>
          <%= tag.p f.text_field :email, id: "email" %>
        </div>
        <div>
          <label for="password">Password</label>
          <%= tag.p f.password_field :password, id: "password" %>
        </div>
        <div>
          <label for="password_confirmation">Password Confirmation</label>
          <%= tag.p f.password_field :password_confirmation, id: "password_confirmation" %>
        </div>
        <div>
          <%= f.submit "Submit" %>
        </div>
      <% end %>
    </fieldset>
  <% end %>

</section>